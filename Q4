int a0,a1,a2;
        a0=a1=a2=0;
        for(int i=0;i<n;i++)
        {
            if(a[i]==0)
                a0++;
            else if(a[i]==1)
                a1++;
            else
                a2++;
        }
        for(int i=0;i<a0;i++)
            a[i]=0;
        for(int i=a0;i<a0+a1;i++)
            a[i]=1;
        for(int i=a0+a1;i<n;i++)
            a[i]=2;
