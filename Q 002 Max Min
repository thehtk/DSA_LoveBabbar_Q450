#include <iostream>
using namespace std;


int main()
{
    int a[10]={5,7,19,4,10,6,70,8,3,4};
    int n=10;
    int max,min;
    max=min=a[0];
    for(int i=0;i<n;i++)
    {
        if(a[i]<min)
            min=a[i];
        if(a[i]>max)
            max=a[i];
    }
    cout<<" Max element "<<max;
    cout<<endl<<" Min element "<<min;
    

    return 0;
}
